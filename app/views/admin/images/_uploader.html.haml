#uploadImagesModal.image-upload-modal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "uploadImagesModal", role: "dialog", tabindex: "-1"}
  .modal-dialog.modal-lg{role: "document"}
    .modal-content
      .modal-header
        %h5.modal-title Images uploader
        %button.close{"aria-label" => "Close", "data-dismiss" => "modal", type: "button"}
          %span{"aria-hidden" => "true"} Ã—
      .modal-body
        .images-container
          = render 'admin/images/images', images: Image.all
        .images-upload-form
          = render 'admin/images/form', image: Image.new
      .modal-footer
        %button.btn.btn-secondary{"data-dismiss" => "modal", type: "button"} Close
        %button.btn.btn-primary{type: "button", "data-dismiss" => "modal"} Save changes